{
    "name": "agegate/common",
    "type": "library",
    "description": "Common functionality used by Age Gate.",
    "keywords": [],
    "license": "MIT",
    "homepage": "https://asylum.digital",
    "repositories": {
        "asylum/utility": {
            "type": "vcs",
            "url": "git@github.com:philsbury/asylum-utility.git"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "adbario/php-dot-notation": "^3.3",
        "erusev/parsedown": "^1.7",
        "illuminate/collections": "^8.83",
        "jawira/case-converter": "^3.6",
        "jaybizzle/crawler-detect": "^1.3",
        "league/plates": "^3.5",
        "ramsey/uuid": "^4.2",
        "asylum/utility": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "AgeGate\\Common\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "symfony/var-dumper": "^5.4"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
